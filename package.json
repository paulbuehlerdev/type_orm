{
  "name": "@beshu-tech/authfish_type_orm",
  "version": "1.0.16",
  "description": "Trying TypeORM with BSL3MC",
  "scripts": {
    "build": "tsc --noEmitOnError",
    "start": "npx nodemon src/app.ts",
    "start:dist": "node dist/app.js",
    "test": "jest",
    "test:ci": "jest --ci",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "npm run typeorm migration:create ./src/migrations",
    "migration:generate": "npm run typeorm migration:generate -- -d ./src/db/data-source.ts --pretty",
    "migration:show": "npm run typeorm migration:show -- -d ./src/db/data-source.ts",
    "migration:run": "npm run typeorm migration:run -- -d  ./src/db/data-source.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d ./src/db/data-source.ts"
  },
  "dependencies": {
    "@types/node": "^20.11.16",
    "better-sqlite3": "^9.4.0",
    "better-sqlite3-multiple-ciphers": "^9.4.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "reflect-metadata": "^0.2.1",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.20",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": "20.x"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/jest": "^29.5.11",
    "@types/node-fetch": "^2.6.11",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "private": false,
  "files": [
    "dist/**/*.js",
    "**/*.ts",
    "certs/*.pem",
    "models/*.js",
    "public/*",
    "settings.yml"
  ]
}
